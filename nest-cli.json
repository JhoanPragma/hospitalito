{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/gateway/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/gateway/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/gateway",
  "projects": {
    "gateway": {
      "type": "application",
      "root": "apps/gateway",
      "entryFile": "main",
      "sourceRoot": "apps/gateway/src",
      "compilerOptions": {
        "tsConfigPath": "apps/gateway/tsconfig.app.json"
      }
    },
    "pharmacy": {
      "type": "application",
      "root": "apps/pharmacy",
      "entryFile": "main",
      "sourceRoot": "apps/pharmacy/src",
      "compilerOptions": {
        "tsConfigPath": "apps/pharmacy/tsconfig.app.json"
      }
    },
    "prescription": {
      "type": "application",
      "root": "apps/prescription",
      "entryFile": "main",
      "sourceRoot": "apps/prescription/src",
      "compilerOptions": {
        "tsConfigPath": "apps/prescription/tsconfig.app.json"
      }
    },
    "medical-history": {
      "type": "application",
      "root": "apps/medical-history",
      "entryFile": "main",
      "sourceRoot": "apps/medical-history/src",
      "compilerOptions": {
        "tsConfigPath": "apps/medical-history/tsconfig.app.json"
      }
    },
    "patient-registration": {
      "type": "application",
      "root": "apps/patient-registration",
      "entryFile": "main",
      "sourceRoot": "apps/patient-registration/src",
      "compilerOptions": {
        "tsConfigPath": "apps/patient-registration/tsconfig.app.json"
      }
    }
  }
}